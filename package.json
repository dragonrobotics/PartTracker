{
  "name": "partstracker",
  "version": "0.1.0",
  "description": "Parts and system assembly tracking web application",
  "main": "index.js",
  "scripts": {
    "build": "browserify client-js/main.jsx -t babelify -o static/js/bundle.js --debug",
    "start": "node index.js",
    "start-db": "mongod --dbpath ./data/db --port 27017",
    "postinstall": "mkdir -p data/db",
    "test": "concurrently -c \"green,cyan\" --kill-others \"npm run-script start-db\" \"npm run-script start\""
  },
  "author": "Sebastian Mobo <stmobo@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "app-module-path": "^2.2.0",
    "body-parser": "^1.17.1",
    "bootstrap": "^3.3.7",
    "concurrently": "^3.4.0",
    "connect-ensure-login": "^0.1.1",
    "express": "^4.15.2",
    "express-session": "^1.15.3",
    "monk": "^4.0.0",
    "passport": "^0.3.2",
    "passport-http": "^0.3.0",
    "passport-local": "^1.0.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^14.3.0"
  },
  "babel": {
    "presets": [
      "react",
      "es2015"
    ]
  }
}
