{
  "name": "partstracker",
  "version": "0.2.0",
  "description": "Parts and system assembly tracking web application",
  "repository": {
    "type": "git",
    "url": "https://github.com/stmobo/PartTracker.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "gulp build",
    "start": "node index.js",
    "start-local": "node index.js --pid_file server.pid --log_file server.log --no_https --http_port 3000",
    "start-db": "mongod --dbpath ./data/db --port 27017",
    "postinstall": "mkdir -p data/db acme-static/.well-known/acme-challenge public/js static/js",
    "test": "concurrently -c \"green,cyan\" --kill-others \"npm run-script start-db\" \"npm run-script start\""
  },
  "author": "Sebastian Mobo <stmobo@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "app-module-path": "^2.2.0",
    "body-parser": "^1.17.1",
    "bootstrap": "^3.3.7",
    "jquery": "^3.2.1",
    "concurrently": "^3.4.0",
    "connect-ensure-login": "^0.1.1",
    "express": "^4.15.2",
    "express-session": "^1.15.3",
    "helmet": "^3.6.1",
    "minimist": "^1.2.0",
    "monk": "^4.0.0",
    "npid": "^0.4.0",
    "passport": "^0.3.2",
    "passport-http": "^0.3.0",
    "passport-local": "^1.0.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "underscore": "^1.8.3",
    "winston": "^2.3.1",
    "winston-syslog": "^1.2.6"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^14.3.0",
    "gulp": "^3.9.1",
    "gulp-streamify": "^1.0.2",
    "gulp-uglify": "^3.0.0",
    "gulp-util": "^3.0.8",
    "pump": "^1.0.2",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.9.0"
  },
  "babel": {
    "presets": [
      "react",
      "es2015"
    ]
  }
}
